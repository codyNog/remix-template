{
  "name": "my-remix-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "lint": "run-p lint:*",
    "lint:script": "eslint --max-warnings=0 './src/**/*.tsx' './src/**/*.ts'",
    "lint:format": "prettier --check ./src",
    "lint:markdown": "markdownlint-cli2 '**/*.md' !node_modules",
    "lint:markup": "markuplint -p src/**/*.tsx",
    "lint:spell": "cspell --quiet .",
    "test": "vitest run ./src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier ./src --write",
    "plop": "plop",
    "e2e": "playwright test",
    "depcheck": "depcheck",
    "gen": "run-p gen:*",
    "gen:i18n": "mkdir -p gen/i18n && bun ./i18n/index.js"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "valibot": "^0.27.1"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^3.12.0",
    "@markuplint/react-spec": "^3.13.0",
    "@next/eslint-plugin-next": "^14.0.3",
    "@playwright/test": "^1.40.1",
    "@remix-run/dev": "^2.5.1",
    "@storybook/addon-actions": "^7.6.3",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/blocks": "^7.6.3",
    "@storybook/builder-vite": "^7.6.3",
    "@storybook/nextjs": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/testing-react": "^2.0.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^4.2.1",
    "cspell": "^8.1.2",
    "depcheck": "^1.4.7",
    "eslint": "^8.38.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unicorn": "^49.0.0",
    "jsdom": "^23.0.1",
    "lefthook": "^1.5.5",
    "markdownlint-cli2": "^0.11.0",
    "markuplint": "^3.15.0",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.4.1",
    "plop": "^4.0.0",
    "prisma": "5.6.0",
    "react-docgen-typescript-plugin": "^1.0.5",
    "storybook": "^7.6.3",
    "storycap": "^4.2.0",
    "typescript": "^5.3.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
